{
  "name": "@tubular/array-buffer-reader",
  "version": "2.1.0",
  "description": "Read from an array or ArrayBuffer like a binary input stream.",
  "browser": "dist/web/index.js",
  "main": "dist/es5/index.js",
  "module": "dist/index.js",
  "esm2015": "dist/index.js",
  "typings": "dist/index",
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist/ && tsc && tsc --build tsconfig.es5.json && webpack",
    "prepublishOnly": "npm run build",
    "lint": "eslint 'src/**/*.ts' '**/*.cjs'",
    "test": "nyc --reporter=html mocha --require ts-node/register src/**/*.spec.ts"
  },
  "keywords": [
    "array",
    "arraybuffer",
    "binary",
    "data",
    "stream"
  ],
  "author": "Kerry Shetline <kerry@shetline.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/register": "^7.13.16",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "eslint": "^7.26.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-chai-friendly": "^0.7.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jasmine": "^4.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-node-externals": "^3.0.0"
  },
  "repository": "github:kshetline/tubular_array-buffer-reader"
}
